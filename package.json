    "@zeit/next-less": "^1.0.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "lint-staged": "^11.0.0",
